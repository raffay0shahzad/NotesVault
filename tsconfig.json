{
  "extends": "@react-native/typescript-config",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "#src/*": ["./src/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "declarations.d.ts",
    "nativewind-env.d.ts",
    "src/types/react-native-config.d.ts"
  ],
  "exclude": ["**/node_modules", "**/Pods"]
}
